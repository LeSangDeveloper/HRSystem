@*
    若路徑不對, 找不到對英的網頁, 都跳到這裡
*@
@page
@{
    Layout = "_ErrorLayout";
    ViewBag.Title = "404 - Page Not Found";
}

<h1 class="text-danger">404</h1>
<h3>Page Not Found</h3>

<p class="text-muted">
    The page you are looking for does not exist.
</p>

<a asp-page="/Index" class="btn btn-primary">
    Go Home
</a>
